    <h3 class="center-text">Edit/Delete Events</h3>
     <% if (events){ %>
      <table class="table table-striped">
      <tbody>
      <% var nowFormat = moment(); %>
      <% events.forEach(function(event){ %>
      <%
        var month = event.date.slice(0,2);
        var day = event.date.slice(3,5);
        var year = event.date.slice(6,10);
        var forecastTime = year + '-' + month + '-' + day;
        var thisEventDate = moment(forecastTime).format('MMMM Do, YYYY');
        var dateForDow = moment(forecastTime);
        var thisDayOfWeek = dateForDow.format('dddd');
        var difference = dateForDow.diff(nowFormat);
%>
<% if(difference > -172800000){ %>
      <tr>
        <td>
          <a attr="<%=event.id%>" href="#" class='delete-link-event'>Delete</a><br>
          <a attr="<%=event.id%>" href="/edit-event/show/<%=event.id%>" class='edit-link-event'>Edit</a>
        </td>
        <td><span style="font-size: 12px;"><%=thisDayOfWeek%></span><br><span style="font-weight: bold;"><%=thisEventDate%></span></td>
        <td><%=event.venue%><br><%=event.city%></td>
      </tr>
      <% } %>
           <% }) %>
      </tbody>
      </table>
      
    <% } else{%>
    <h2>You have no events. :-(</h2>
    <% } %>




















      

    

    
   
   
  




